{
    "json_url": "ts-xapp",
    "xapp_name": "ts-xapp",
    "version": "1.0.0",
    "containers": [
        {
            "name": "ts-xapp",
            "image": {
                "registry": "xApp-registry.local:5008",
                "name": "ts-xapp",
                "tag": "1.0.0"
            }
        }
    ],
    "messaging": {
        "ports": [
            {
                "name": "rmr-data",
                "container": "ts-xapp",
                "port": 4560,
                "rxMessages": [ "TS_QOE_PREDICTION", "A1_POLICY_REQ", "TS_ANOMALY_UPDATE" ],
                "txMessages": [ "TS_UE_LIST", "TS_ANOMALY_ACK" ],
                "policies": [20008],
                "description": "rmr receive data port for ts-xapp"
            },
            {
                "name": "rmr-route",
                "container": "ts-xapp",
                "port": 4561,
                "description": "rmr route port for ts-xapp"
            }
        ]
    },
    "rmr": {
        "protPort": "tcp:4560",
        "maxSize": 2072,
        "numWorkers": 1,
        "txMessages": [ "TS_UE_LIST", "TS_ANOMALY_ACK" ],
        "rxMessages": [ "TS_QOE_PREDICTION", "A1_POLICY_REQ", "TS_ANOMALY_UPDATE" ],
        "policies": [20008]
    },
    "controls": {
        "ts_control_api": "rest",
        "ts_control_ep": "http://127.0.0.1:5000/api/echo"
    }
}
